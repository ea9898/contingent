<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/"
                  xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns:xs="http://www.w3.org/2001/XMLSchema"
                  xmlns:tns="http://emias.mos.ru/exchangeProduct/exchangeService/v1/" name="exchangeService"
                  xmlns:ns1="http://op.mos.ru/receive_changes"
                  targetNamespace="http://emias.mos.ru/exchangeProduct/exchangeService/v1/">
    <wsdl:types>
        <xs:schema>
            <xs:import namespace="http://emias.mos.ru/system/v1/userContext/"
                       schemaLocation="emias.system.v1.userContext.xsd"/>
            <xs:import namespace="http://emias.mos.ru/system/v1/faults/" schemaLocation="emias.system.v1.faults.xsd"/>
            <xs:import namespace="http://op.mos.ru/receive_changes"
                       schemaLocation="exchangeProduct.exchangeService.v1.types.xsd"/>
        </xs:schema>
    </wsdl:types>

    <wsdl:message name="getChangeElement">
        <wsdl:part name="getChangeElement" element="ns1:ChangeElement"/>
    </wsdl:message>
    <wsdl:message name="getResponseElement">
        <wsdl:part name="getResponseElement" element="ns1:ResponseElement"/>
    </wsdl:message>

    <wsdl:portType name="exchangeServicePortType">
        <wsdl:operation name="get">
            <wsdl:input message="tns:getChangeElement" name="getChangeElement"/>
            <wsdl:output message="tns:getResponseElement" name="getResponseElement"/>
        </wsdl:operation>
    </wsdl:portType>

    <wsdl:binding name="getSoap" type="tns:exchangeServicePortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="get">
            <soap:operation soapAction="http://op.mos.ru/receive_changes/receive_change"/>
            <wsdl:input name="getChangeElement">
                <soap:body use="literal"/>
            </wsdl:input>
            <wsdl:output name="getResponseElement">
                <soap:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>

    <wsdl:service name="getService">
        <wsdl:port binding="tns:getSoap" name="getSoap">
            <soap:address location="REPLACE_WITH_ACTUAL_URL"/>
        </wsdl:port>
    </wsdl:service>
</wsdl:definitions>