<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://emias.mos.ru/contingent2/address/v4/"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://emias.mos.ru/contingent2/address/v4/"
           xmlns:restr="http://emias.mos.ru/restriction/types/core/"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified">

    <xs:include schemaLocation="../../emias/v1_2/emias.core.address.v1.xsd"/>
    <xs:import namespace="http://emias.mos.ru/restriction/types/core/" schemaLocation="../v1/emias.restriction.types.core.xsd"/>

    <xs:complexType name="SearchAreaAddress">
        <xs:annotation>
            <xs:documentation>Сведения адреса для метода searchArea</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="globalIdNsi" type="xs:long" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Уникальный идентификатор адреса в ЕМИАС.НСИ</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
</xs:schema>