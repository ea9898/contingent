<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://emias2.gov.ru/contingent/v3/public/common/"
            xmlns:tns="http://emias2.gov.ru/contingent/v3/public/common/"
            xmlns:area="http://emias2.gov.ru/contingent/v3/area/types/"
            xmlns:common="http://emias2.gov.ru/contingent/v1/common/types/"
            elementFormDefault="qualified"
            version="2.0">

    <xsd:import namespace="http://emias2.gov.ru/contingent/v1/common/types/" schemaLocation="common-types.xsd" />
    <xsd:import namespace="http://emias2.gov.ru/contingent/v3/area/types/" schemaLocation="area-types.xsd" />

    <xsd:complexType name="Lpu">
        <xsd:annotation>
            <xsd:documentation>ЛПУ</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="id" type="xsd:long"/>
            <xsd:element name="groupId" minOccurs="0" type="xsd:long"/>
            <xsd:element name="mainLpuId" minOccurs="0" type="xsd:long"/>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="address" minOccurs="0" type="xsd:string"/>
            <xsd:element name="phone" minOccurs="0" type="xsd:string"/>
            <xsd:element name="shortName" type="xsd:string"/>
            <xsd:element name="headFinance" type="xsd:boolean"/>
            <xsd:element name="ogrn" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LpuWithAddressPoint">
        <xsd:annotation>
            <xsd:documentation>ЛПУ</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="id" type="xsd:long"/>
            <xsd:element name="groupId" minOccurs="0" type="xsd:long"/>
            <xsd:element name="mainLpuId" minOccurs="0" type="xsd:long"/>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="address" type="tns:LpuAddress" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="phone" minOccurs="0" type="xsd:string"/>
            <xsd:element name="shortName" type="xsd:string"/>
            <xsd:element name="headFinance" type="xsd:boolean"/>
            <xsd:element name="ogrn" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LpuAddress">
        <xsd:annotation>
            <xsd:documentation>Адрес МО</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="addressPointId" type="xsd:long"/>
            <xsd:element name="address" type="xsd:string"/>
            <xsd:element name="isMain" type="xsd:boolean"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RequestPageInfo">
        <xsd:sequence>
            <xsd:element name="page" type="xsd:int" minOccurs="1" default="0">
                <xsd:annotation>
                    <xsd:documentation>Номер страницы</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="pageSize" type="xsd:int" minOccurs="1" default="0">
                <xsd:annotation>
                    <xsd:documentation>Размер страницы</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ResponsePageInfo">
        <xsd:complexContent>
            <xsd:extension base="tns:RequestPageInfo">
                <xsd:sequence>
                    <xsd:element name="total" type="xsd:long" minOccurs="1" default="0">
                        <xsd:annotation>
                            <xsd:documentation>Всего записей</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="LpuType">
        <xsd:annotation>
            <xsd:documentation>Тип ЛПУ</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:NsiType">
                <xsd:sequence>
                    <xsd:element name="dispensary" type="xsd:boolean"/>
                    <xsd:element name="selfAssign" type="xsd:boolean"/>
                    <xsd:element name="ambulatory" type="xsd:boolean"/>
                    <xsd:element name="shortName" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="NsiType">
        <xsd:annotation>
            <xsd:documentation>Базовый класс для справочников</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="code" type="xsd:string"/>
            <xsd:element name="name" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AgeGroup">
        <xsd:annotation>
            <xsd:documentation>Возрастная группа</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:NsiType">
                <xsd:sequence>
                    <xsd:element name="from" type="xsd:int"/>
                    <xsd:element name="to" type="xsd:int"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="AreaType">
        <xsd:annotation>
            <xsd:documentation>Тип участка</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:NsiType">
                <xsd:sequence>
                    <xsd:element name="headFinance" type="xsd:boolean"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="SocioProfessionalGroupCache">
        <xsd:complexContent>
            <xsd:extension base="tns:NsiType">
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="EthnicGroupCache">
        <xsd:complexContent>
