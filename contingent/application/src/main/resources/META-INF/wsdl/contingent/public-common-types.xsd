    <xsd:complexType name="Specialities">
        <xsd:annotation>
            <xsd:documentation>Специальности</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="speciality" type="xsd:string" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AreaTypeFull">
        <xsd:annotation>
            <xsd:documentation>Тип участка</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:NsiType">
                <xsd:sequence>
                    <xsd:element name="headFinance" type="xsd:boolean"/>
                    <xsd:element name="ageGroup" type="xsd:string" >
                        <xsd:annotation>
                            <xsd:documentation>Возрастная группа участка</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="defaultAttachmentPossibility" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation>Возможность базового прикрепления</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="normAttachment" type="xsd:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Норма прикрепления (чел.)</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="gender" type="area:GenderType"  minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>пол пациентов</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="specialities" type="tns:Specialities">
                        <xsd:annotation>
                            <xsd:documentation>специальность врачей на участке</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="AreaTypeUpk">
        <xsd:annotation>
            <xsd:documentation>Тип участка (база УПК)</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="code" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Идентификатор типа участка</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ageGroup" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Возрастная группа участка</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="defaultAttachmentPossibility" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>Возможность базового прикрепления</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="normAttachment" type="xsd:int">
                <xsd:annotation>
                    <xsd:documentation>Норма прикрепления (чел.)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="gender" type="xsd:int">
                <xsd:annotation>
                    <xsd:documentation>пол пациентов</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="medical" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation>Тип медицинского участка</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="speciality" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>специальность врачей на участке</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


