                            <xsd:documentation>Тип</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="teCode" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Код ТЕ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="tmCode" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Код ТМ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="msrNameOld" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Старое наименование МСР</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="msrName" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Наименование МСР</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="msrCode" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Код улицы МСР</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="btiCode" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Код улицы БТИ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="btiName" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Наименование улицы БТИ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="codeNew" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Код улицы ЕРЗ с новым наименованием</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="codeKladr" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Код КЛАДР</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="OmkTe">
        <xsd:annotation>
            <xsd:documentation>Справочник «ОМК ТЕ»</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:NsiType">
                <xsd:sequence>
                    <xsd:element name="shortName" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Краткое наименование ТЕ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="type" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Вид ТЕ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="okatoCode" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Код ОКАТО</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="okatoKs" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>КС ОКАТО</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="Employee">
        <xsd:annotation>
            <xsd:documentation>Врач в ЛПУ</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="id" type="xsd:long"/>
            <xsd:element name="speciality" minOccurs="0" type="tns:Speciality"/>
            <xsd:element name="name" type="common:FullName"/>
            <xsd:element name="lpuDepartment" type="tns:LpuDepartment" minOccurs="0"/>
            <xsd:element name="snils" type="xsd:string"/>
            <xsd:element name="jobStartDate" type="xsd:date" minOccurs="0"/>
            <xsd:element name="jobEndDate" type="xsd:date" minOccurs="0"/>
            <xsd:element name="isActualJob" type="xsd:boolean" minOccurs="0"/>
            <xsd:element name="isReplacement" type="xsd:boolean" minOccurs="0" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LpuDepartment">
        <xsd:annotation>
            <xsd:documentation>Подразделение МУ</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="id" type="xsd:long"/>
            <xsd:element name="name" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Account">
        <xsd:annotation>
            <xsd:documentation>Информация о пользователе</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="id" type="xsd:long"/>
            <xsd:element name="medicalEmployeeId" type="xsd:long" minOccurs="0"/>
            <xsd:element name="name" type="common:FullName" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Signatory">
        <xsd:annotation>
            <xsd:documentation>Подписант заявления</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="signatoryId" type="xsd:long" minOccurs="0"/>
            <xsd:element name="signatoryPosition" minOccurs="0" type="tns:Post"/>
            <xsd:element name="signatoryFio" minOccurs="0" type="common:FullName"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Address">
        <xsd:annotation>
            <xsd:documentation>Адрес участка</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="city" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Город</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="house" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Дом</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="building" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Корпус</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="construction" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Строение</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="apartments" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Квартира</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="omkUmCode" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Код улицы ОМК</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="omkUmName" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Название улицы ОМК</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="omkTeRegionCode" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Код округа ОМК</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="omkTeRegionName" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Название округа ОМК</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="omkTeDistrictCode" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Код района ОМК</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="omkTeDistrictName" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Название района ОМК</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GeoAddress">
        <xsd:annotation>
            <xsd:documentation>Адрес</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:Address">
                <xsd:sequence>
                    <xsd:element name="geoLatLon" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Широта;Долгота</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="Position">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:string" />
            <xsd:element name="shortName" type="xsd:string" minOccurs="0" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="User">
        <xsd:sequence>
            <xsd:element name="accountId" type="xsd:long" minOccurs="0" />
            <xsd:element name="userLogin" type="xsd:string" minOccurs="0" />
            <xsd:element name="name" type="common:FullName" minOccurs="0" />
            <xsd:element name="position" type="tns:Position" minOccurs="0" />
            <xsd:element name="userLpu" type="tns:Lpu" minOccurs="0" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="UserRole">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:long" />
            <xsd:element name="name" type="xsd:string" minOccurs="0" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LpuShort">
        <xsd:annotation>
            <xsd:documentation>ЛПУ</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="id" type="xsd:long"/>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="shortName" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Warning">
        <xsd:annotation>
            <xsd:documentation>Предупреждение</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="code" type="xsd:string"/>
            <xsd:element name="message" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

</xsd:schema>