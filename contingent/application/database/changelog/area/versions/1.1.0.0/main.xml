            </and>
        </preConditions>
        <comment>Настройка безопасности методов web-сервиса</comment>
        <loadData catalogName="cat"
            encoding="UTF-8"
            file="changelog/area/versions/1.1.0.0/files/security.config.csv"
            quotchar=""
            separator="#"
            tableName="AD_CONFIG">
            <column name="code" type="STRING"/>
            <column name="val" type="STRING"/>
        </loadData>
    </changeSet>

